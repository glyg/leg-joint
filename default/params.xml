<?xml version="1.0"?>
<parameters>
  <param max="30" min="0" name="n_sigmas" step="1" value="7">
    <unit>um</unit>
    <description>Initial number of cell along sigma </description>
  </param>
  <param max="30" min="0" name="n_zeds" step="1" value="5">
    <unit>um</unit>
    <description>Initial number of cell along zed</description>
  </param>
  <param max="10." min="0.1" name="lambda_0" step="0.1" value="4.3">
    <unit>um</unit>
    <description>Average cell to cell distance</description>
  </param>
  <param max="100." min="0.1" name="rho0" step="0.1" value="13.">
    <unit>um</unit>
    <description>epithelium radius</description>
  </param>
  <param max="100." min="0.1" name="rho_lumen" step="0.1" value="4.">
    <unit>um</unit>
    <description>lumen radius</description>
  </param>
  <param max="200." min="0.1" name="cell_volume" step="0.1" value="80.">
    <unit>um3</unit>
    <description>cell volume</description>
  </param>
  <param max="10." min="0." name="pos_noise" step="0.01" value="0.01">
    <unit>um</unit>
    <description>vertex position noise</description>
  </param>
  <param max="10." min="0.001" name="pos_cutoff" step="0.0001" value="1e-35">
    <unit>um</unit>
    <description>Distance cut-off </description>
  </param>
  <param max="10." min="0.1" name="theta_cutoff" step="0.01" value="0.01">
    <unit>um</unit>
    <description>Angular cut-off </description>
  </param>
  <param max="10." min="0.1" name="elasticity" step="0.01" value="1.">
    <unit>pN/um2</unit>
    <description>Area elasticity</description>
  </param>
  <param max="50." min="0.1" name="prefered_area" step="0.01" value="12.">
    <unit>um2</unit>
    <description>Prefered area</description>
  </param>
  <param max="10." min="0.001" name="contractility" step="0.01" value="0.04">
    <unit>adim</unit>
    <description>Cell contractility</description>
  </param>
  <param max="10." min="-10." name="line_tension" step="0.01" value="0.12">
    <unit>adim</unit>
    <description>Line tension</description>
  </param>
  <param max="10." min="0." name="radial_tension" step="0.01" value="0.1">
    <unit>adim</unit>
    <description>Apical-basal tension</description>
  </param>
  <param max="1." min="0." name="vol_elasticity" step="0.001" value="1.">
    <unit>adim</unit>
    <description>Volume elasticity</description>
  </param>
</parameters>
